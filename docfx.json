{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "src": "docs-conceptual",
        "version": "sccm-ps",
        "dest": "sccm",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
    {
      "files": [
        "toc.yml"
      ],
      "src": "docs-conceptual",
      "version": "sccm-ps",
      "dest": "sccm/sccm"
    },
    {
      "files": [
        "**/*.md"
      ],
      "src": "sccm-ps",
      "version": "sccm-ps",
      "dest": "module"
    },
    {
      "files": [
        "toc.yml"
      ],
      "src": "sccm-ps",
      "version": "sccm-ps",
      "dest": "module/sccm-ps"
    },
    {
      "files": [
        "**/*.md"
      ],
      "src": "virtual-directory",
      "dest": "sccm"
    },
    {
      "files": [
        "toc.yml"
      ],
      "src": "bread",
      "version": "sccm-ps",
      "dest": "sccm/bread"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],

    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "apiPlatform": "powershell",
      "breadcrumb_path": "/powershell/sccm/bread/toc.json",
	  "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/sccm-docs-powershell-ref",
      "feedback_product_url": "https://configurationmanager.uservoice.com"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "sccm-docs-powershell-ref",
    "markdownEngineName": "markdig"
  }
}
